/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "strictFunctionTypes": false,
    "baseUrl": "./",
    "sourceRoot": "/",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": ["src/models", "node_modules/@types"],
    "lib": ["es2018", "dom"],
    "skipLibCheck": true
  }
}
